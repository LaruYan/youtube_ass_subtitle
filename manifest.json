{
    // Required
    "manifest_version": 2,
    "name": "youtube ass subtitle",
    "version": "0.1",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "image/16.png",
            "32": "image/32.png",
            "64": "image/64.png"
        },
        "page_url": "https://*.youtube.com/*"
    },
    "background": {
        "persistent": false,
        "scripts": [ "src/worker_proxy/worker_proxy.js" ]
        // "scripts": [ "src/worker_proxy/worker_proxy.js" ]
    },
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": [ "src/worker_proxy/worker_proxy.js", "src/subtitles_octopus/subtitles-octopus.js", "src/loader.js"]
        }
    ],
    "web_accessible_resources": ["worker_proxy.html"],
    "permissions": [ "https://*.youtube.com/*" ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens hello.html"
        }
    },
    "icons": {
        "16": "image/16.png",
        "32": "image/32.png",
        "64": "image/64.png"
    }
